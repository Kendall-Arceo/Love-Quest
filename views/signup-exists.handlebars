<div class="container">
    <div class="row">
        <h5>The quest for love awaits</h5>
    </div>

    <div class="card col-10 d-flex justify-content-center">
        <h5 class="card-header">New Love information needed!</h5>
        <div class="card-body">
            <form class=".sign-up-form">
                <div class="form-group">
                    <label for="exampleInputEmail1">Username</label>
                    <input type="email" class="username-signup" id="exampleInputEmail1" aria-describedby="emailHelp"
                        placeholder="Username" />
                </div>
                <div class="username-valid"></div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Password</label>
                    <input type="password" class="password-signup" id="exampleInputPassword1" placeholder="Password" />
                </div>
                <div class="password-valid"></div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Confirm Password</label>
                    <input type="password" class="password-confirm" id="exampleInputPassword1"
                        placeholder="Confirm Password" />
                </div>
                <div class="ps-valid"></div>
                <div class="form-group">
                    <label for="exampleInputPassword1">Date of Birth</label>
                    <input type="password" class="date-of-birth" id="exampleInputPassword1"
                        placeholder="Date of Birth YYYY-MM-DD" />
                </div>
                <div class="dob-valid"></div>

                <button type="submit" class="btn btn-primary">Submit</button>
            </form>
        </div>
        <div class="exists">Account already exists</div>
    </div>
</div>

<div class="fixed-bottom">Footer!</div>

